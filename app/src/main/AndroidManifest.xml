<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.pdf">

    <uses-sdk
        android:minSdkVersion="INSERT_YOUR_DESIRED_minSdkVersion_HERE"
        tools:overrideLibrary="com.paypal.android.sdk.payments" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="false"
        android:theme="@style/Theme.PDF"
        tools:targetApi="31">
        <activity
            android:name="com.example.pdf.Pay"
            android:exported="false" />
        <activity
            android:name="com.example.pdf.Payment"
            android:exported="false" />
        <activity
            android:name="com.example.pdf.Math"
            android:exported="false" />
        <activity
            android:name="com.example.pdf.Quizs"
            android:exported="false" />
        <activity
            android:name="com.example.pdf.MathQuestions"
            android:exported="false" />
        <activity
            android:name="com.example.pdf.WrittenQuestions"
            android:exported="false" />
        <activity
            android:name="com.example.pdf.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pdf.MainActivity" />
    </application>

</manifest>